<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="goodee.gdj58.booking_c.mapper.customer.BookingCompanyOffdayMapper">
	<select id="bookingCompanyOffday" parameterType="String" resultType="java.util.Map">
		SELECT bkcOff.booking_company_offday_date bkcOff
		FROM booking_company_offday bkcOff
		WHERE bkcOff.booking_company_id = #{bkcId}
	</select>

</mapper>